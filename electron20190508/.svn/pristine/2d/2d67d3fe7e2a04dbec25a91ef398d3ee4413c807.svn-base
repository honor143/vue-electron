    /* ElementUi 表格样式修改 */

    .el-table{
        background-color: rgba(66,66,66,0.2);
        font-size: 12px;
    }
    .el-table__empty-block {
        min-height: 60px;
        text-align: center;
        height: 100%;
        display: flex;
        -webkit-box-pack: center;
        justify-content: center;
        -webkit-box-align: center;
        align-items: center;
    }
    .el-table::before,.el-table::after,.el-table__fixed-right::before{
        width: 0;
        height: 0;
    }
    .el-table .my-el-header th, .el-table .my-el-header tr{
        height: 24px;
        color: #eee;
        padding: 9px;
        font-size: 12px;
        background-color: #232426;
        border: none;
    }
    .el-table th,.el-table tr{
        background: transparent;
    }    

    .el-table--border, .el-table--group{
        border: none;
    }
    .el-table__body .el-table__row td{
        background-color: rgba(38,39,41,0.1);
        height: 24px;
        padding: 0;
        color: #FFEC8B;
        font-size: 12px;
        border-bottom: none;
    }
    .el-table--striped .el-table__body tr.el-table__row--striped td{
        background-color: rgba(38,39,41,0.9);
        border-bottom: none;
    }
    .el-table__body tr.hover-row>td,.el-table__body tr.el-table__row--striped.hover-row td{
        background:rgb(80, 41, 3);
    }
    .el-table__fixed-right{
        background:#181818;
        top: 0;
        left: auto;
        right: 0;
        position: absolute;
    }
    .el-loading-mask{
        background: #212121;
    }
    .el-table--striped .el-table__body tr.el-table__row--striped.current-row td,
