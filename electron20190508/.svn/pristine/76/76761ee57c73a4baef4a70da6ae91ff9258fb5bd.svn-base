@import "./basevariable.scss";
em{
    font-style: normal;
}
/* 预警色 */
.warning-pass{
    color:$successcolor;
}
.warning-stop{
    color:$waringcolor;
}
/* 可编辑行 */
.editable-td-class{
    background-color: #2D8CF0!important;
}
/* flex */
.dis-flex{
    display:flex;
}
/* 公共的页面wrapper */
.bond-info{
    width: 100%;
    height:100%;  
    overflow: hidden;  
}
/* 对齐 */
.align-left{
    text-align: left;
}
.align-center{
    text-align: center;
}
.align-right{
    text-align: right;
}
/* 外边距 */
.marbot5{
    margin-bottom: 5px;
}
.marbot10{
    margin-bottom: 10px;
}
/*内边距 */
.padtop5{
    padding-top:5px;
}
.padright5{
    padding-right:5px;
}
/*modal外边距及样式*/
.modal-wraper{
    padding:10px;
    color: #fff;
    text-align: center;
}
/* 模块标题样式 */
h3{
    padding-left: 5px;
    color: #EE8400;
}
h3 span{
    position: absolute;
    right:5px;
    top:2px;
    font-size: 12px;
    color:#2d8cf0;
    cursor: pointer;
}
/* 高亮显示的样式*/
.waring-style{
    color: #EE8400!important;
}
/*省略号*/
.elipse{
    display:inline-block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    vertical-align: middle;
    margin-top:-3px;
}
/* 省略号宽度 */
.wid280{
    width:280px;
}
.wid120{
     width:55%;
}
.wid110{
    width:110px;
}
.wid90{
    width:90px;
}
.wid80{
    width:65px;
}
.wid70{
    width:70px;
}
.wid50{
    width:calc(100% - 83px); 
}
/* 徽标样式*/
h3 span.ivu-badge{
    right: -3px;
}
.ivu-badge-count{
    left:-2px;
    top:-5px;
    width: 14px;
    min-width: 14px;
    height: 14px;
    line-height: 14px;
    text-align: center;
    padding: 0;
}
/*模块header*/
.sec-header{
    color: #fff;
    position: relative;
    height: 30px;
    // overflow: hidden;
}
// .sec-header h3{
//     height: 30px;
//     line-height: 30px;
//     margin-right: 30px;
//     display: inline-block;
// }

.sec-header h3{
    position: relative;
    display: inline-block;
    padding: .3em 0.6em 0;
    transform: translateY(0);
    // color: rgb(255,235,159);
    color:#EE8400;
    font-size:14px!important;
    font-weight: normal;
}
.sec-header h3::before{
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    bottom: -3px;
    left: 0;
    z-index: -1;
    // background: rgb(26,68,68);
    background: #383838;
    color:rgb(255,235,200);
    // background-image: linear-gradient(hsla(0,0%,100%,.6),hsla(0,0%,100%,0));
    border: 1px solid rgba(0,0,0,.4);
    border-bottom: none;
    border-radius: .5em .5em 0 0;
    transform: perspective(.9em) rotateX(5deg);
    transform-origin: left;
}
.sec-header .align-center{
    position: absolute;
    top:0;
    z-index: 30;
}
.right-wrapper{
    position: absolute;
    right:8px;
    top:0;
    text-align: right;
}
/* ivu-page */
.ivu-page{
    background-color: rgba(66,66,66,0.1);
    padding: 5px;
}
/* bonddetail 样式 */
.detail-wrpper-sty{
    background-color: rgba(66,66,66,0.1);
    height: 196px;
    margin-top: 9px;
    padding:5px;
}


/* 权限相关页面高度 */
.table-wrapper-height{/*table高度*/
    height:calc(100vh - 200px);
}
.table-wrapper,.table-wrapper .ivu-table-wrapper{
    height: 100%;
}
// .table-wrapper{
//     height:calc(100% - 31px);
// }
/*表格高亮样式*/
.waring-row td{
    background-color: rgba(103,36,3,1)!important;
    // color:#fff!important;
}

/*中标确认页面汇总上下表格联动*/
.table-footer>.ivu-table{
    overflow-y:hidden;
    overflow-x: auto!important;
}
.table-footer>.ivu-table>.ivu-table-header{
    overflow: visible;
}
.table-footer>.ivu-table>.ivu-table-tip{
    overflow-x:visible!important;
    overflow-y:visible!important;
}
.table-footer .ivu-table-overflowX{
    overflow-x: visible!important;
}
.table-footer .ivu-table-footer .ivu-table{
    overflow: visible!important;
}
.table-footer .ivu-table-footer .ivu-table-overflowX{
    overflow-x: visible!important;
}
.table-footer .ivu-table-footer .ivu-table-overflowY{
    overflow-y: visible!important;
}

 
/* 加载状态样式 */
.ivu-spin-fix {
    border: 0px solid #dcdee2;
    background-color: #515a6e;
    opacity: 0.95;
    //filter: blur(2px);
}
.demo-spin-icon-load{
    animation: ani-demo-spin 1s linear infinite;
}
@keyframes ani-demo-spin {
    from { transform: rotate(0deg);}
    50%  { transform: rotate(180deg);}
    to   { transform: rotate(360deg);}
}

/*tree懒加载*/
.ivu-scroll-container {
    overflow-y: auto!important;
}
.ivu-scroll-content{
    margin-top:-10px;
}

/* 权限tab样式 */
.role-tab-wrapper .ivu-tabs.ivu-tabs-card>.ivu-tabs-bar .ivu-tabs-tab-active,
.role-tab-wrapper .ivu-tabs.ivu-tabs-card>.ivu-tabs-bar .ivu-tabs-tab-focused,
.role-tab-wrapper .ivu-tabs.ivu-tabs-card>.ivu-tabs-bar .ivu-tabs-tab{
    background: none; 
    border: none;
}
.role-tab-wrapper  .ivu-tabs>.ivu-tabs-bar .ivu-tabs-nav{
    margin-left:5px!important;
}
.role-tab-wrapper  .ivu-tabs-tab{
    position: relative;
    display: inline-block;
    margin-left: -5px!important;
    /* padding: .2em 2em 0!important; */
    padding: .1em 0.8em .1em 0.6em!important;
    transform: translateZ(0);
    background: none;
    border: none;
    color: rgb(255,235,200)!important;
    font-weight: bold;
}
.role-tab-wrapper .ivu-tabs-tab::before{
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: -1;
    background: rgba(102, 102, 102, 0.7);
    border: 1px solid rgba(0,0,0,.4);
    border-radius: 3px;
    border-bottom: none;
    transform:scaleY(1.1) perspective(.5em) rotateX(5deg) translateZ(0);
    /* -webkit-transform:scaleY(1.1) perspective(.5em) rotateX(5deg) translateZ(0);
    transform-origin:left right; */
    -webkit-transform:scaleY(1.1) perspective(.9em) rotateX(4deg) translateZ(0);
    transform-origin:left;
}
.role-tab-wrapper .ivu-tabs-tab:hover{
    color: #000!important;
}
.role-tab-wrapper .ivu-tabs-tab:hover::before{
    background:#EE8400;    
}

.role-tab-wrapper .ivu-tabs.ivu-tabs-card>.ivu-tabs-bar .ivu-tabs-tab-active::before,
.role-tab-wrapper .ivu-tabs.ivu-tabs-card>.ivu-tabs-bar .ivu-tabs-tab-focused::before{
    background:#EE8400;
}

// 卡片背景
.card-bg{
    background:rgba(38,39,41,0.4);
}

//ivue-input-number
.ivu-input-number{
    background-color:#333;
    border-color: #444;
    box-shadow:1px 1px 1px 0px #000 inset;
}
.ivu-input-number:focus,.ivu-input-number:hover{  
    border-color: #333;
}
.ivu-input-number-input{
    background-color:#333;   
    color: #fff;   
}
.ivu-input-number-input:focus,.ivu-input-number-input:hover{    
    box-shadow:1px 1px 1px 2px #111 inset;  
}